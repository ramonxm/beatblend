{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ],
  "baseUrl": "./src",
  "paths": {
    "@app/*": [
      "app/*"
    ],
    "@styles/*": [
      "styles/*"
    ]
  }
}
